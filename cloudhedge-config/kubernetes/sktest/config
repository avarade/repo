kind: Deployment
apiVersion: extensions/v1beta1
metadata:
  name: sktest
  namespace: 1539065724614-sktest-sktest
spec:
  replicas: 1
  template:
    metadata:
      labels:
        deploy: sktest
        cbservice: sktest
    spec:
      containers:
        - name: sktest
          image: gurunath/colortest
          env:
            - name: color
              value: red
            - name: CB_ENV_NAME
              value: 1539065724614-sktest-sktest
            - name: CB_CLUSTER_ID
              value: 5bb74b93b24e40000efd9cd5

---
kind: Service
apiVersion: v1
metadata:
  labels:
    service: sktest
    cbservice: sktest
  name: sktest
  namespace: 1539065724614-sktest-sktest
spec:
  type: LoadBalancer
  ports:
    - port: 80
      targetPort: 80
      name: port80
  selector:
    deploy: sktest

